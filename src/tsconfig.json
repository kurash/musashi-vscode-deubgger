{
	"compilerOptions": {
		"target": "es5",
		"inlineSourceMap": true,
		"noImplicitAny": false,
		"removeComments": true,
		"preserveConstEnums": false,

        "outDir"             : "../out",
        "module"             : "commonjs",
        "moduleResolution"   : "node",
        "allowUnreachableCode": true
	}
}
